/* pages/user/user.wxss */
page {
  height: 100%;
}
.cover-view{
  background-image: url('data:image/jpeg;base64,');
  height: 550rpx;
  opacity: 0.8; 
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center; 
  filter: blur(2px);
  
}
.bg-opacity{
  /* opacity: 0 ; */
  color: white;
  font-weight: bold;
  background-color: #F7D04F;

}
.user-info{
  position: absolute;
  top: 15%;
  left: 37%;
  text-align: center;
  color: black;
}
.user-info view{
  margin: 30rpx auto;
}
.user-name{
  font-size: 36rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  padding: 0;
  box-sizing: border-box;
  background-color: #F2F2F2
}


page {
  background-color: #ddd;
}

.box{
  width: 95%;
  margin: 0 auto 20rpx;
  border-radius:20rpx;

}


.grid-icons{
  display: flex;
  text-align: center;
  position: relative;
  top: -100rpx;
  margin-bottom: -80rpx;
}

.icons-item{
  flex: 1;
  width: 100rpx;
}

.icons-item view:last-child{
 padding-top: 10rpx;
}